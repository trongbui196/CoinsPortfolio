{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
"redirects": [
    {
      "source": "/(.*)",
      "destination": "/",
      "permanent": false,
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "yourdomain.com"
        }
      ]
    }
  ]
}
